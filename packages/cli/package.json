  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./bin/mcpn.mjs"
  },
  "types": "./dist/index.d.ts",
  "bin": {
    "mcpn": "bin/mcpn.mjs"
  },
  "files": [
    "dist",
    "bin"
