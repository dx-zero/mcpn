{
  "name": "mcpn",
  "version": "0.1.0",
  "description": "MCP workflow orchestration and registry tool with presets for thinking, coding and more",
  "type": "module",
  "keywords": [
    "mcp",
    "model-context-protocol",
    "workflows",
    "prompts",
    "prompt engineering",
    "ai",
    "mcpn",
    "mcp network"
  ],
  "contributors": [
    {
      "name": "Ted Werbel",
